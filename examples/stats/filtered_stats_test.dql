# This is the root query to fetch all proper counts
#
# TODO: Check whether this is nicer handled 
# using group by clauses
{  
    iob as var(func: has(InfoObject.sdgs)) {
        uid
    }

    var(func: uid(iob)) @groupby(InfoObject.sdgs)
    {
        tnum as count(uid)
    }

    oss(func: uid(tnum)) {
        val(tnum)
        Sdg.id
    }
}